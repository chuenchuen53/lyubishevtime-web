{
  "plugins": ["prettier-plugin-tailwindcss"],
  "tailwindFunctions": ["tv"],
  "tabWidth": 2,
  "printWidth": 150,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "ignore",
  "useTabs": false,
  "endOfLine": "lf"
}
